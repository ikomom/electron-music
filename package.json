{
  "name": "electron-vue-project",
  "version": "0.1.0",
  "private": true,
  "appId": "com.ikonon.cloudMusic",
  "main": "background.js",
  "author": "ikonon",
  "description": "electron-vue 仿制网易云",
  "scripts": {
    "electron:debug": "vue-cli-service electron:serve --debug",
    "electron:main:debug": "electron --remote-debugging-port=9223 ./dist/electron",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "vue:lint": "vue-cli-service lint",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:build": "vue-cli-service electron:build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "svgo": "svgo -f src/renderer/assets/svg --config=svgo.yml"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.1",
    "rbush": "^3.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-wait": "^1.4.8",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "electron": "^11.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "sass": "^1.32.12",
    "sass-loader": "^10.1.1",
    "svg-sprite-loader": "^6.0.6",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "no-debugger": "warn",
      "no-unused-vars": "warn"
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
